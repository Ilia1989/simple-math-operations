package work;

/**
 * @author Ilko
 *
 */
public class Values {

	private int a = 16;
	private int b = 4;
	private double c = 21.6;
	private double d = 7.2;
	
	
	public Values() {

	}

	public Values(int a, int b, double c, double d) {
		super();
		this.a = a;
		this.b = b;
		this.c = c;
		this.d = d;
	}

	public int getA() {
		return a;
	}

	public void setA(int a) {
		this.a = a;
	}

	public int getB() {
		return b;
	}

	public void setB(int b) {
		this.b = b;
	}

	public double getC() {
		return c;
	}

	public void setC(double c) {
		this.c = c;
	}

	public double getD() {
		return d;
	}

	public void setD(double d) {
		this.d = d;
	}
}
